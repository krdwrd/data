	function popUpPage(sPage,iWd,iHg){
		var obWin;
		//alert(sPage);
		obWin = window.open(sPage,"popWin","width="+iWd+", height="+iHg+",statusbar=notoolbar=0,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=1");
		if(!obWin.opener) obWin.opener = _self;
		obWin.focus();
	}
	
	function cruiseinfo(c)
	{
		//alert(c);
		//alert(document.getElementById("overview").display);
		if (c=="overview")
		{
			document.getElementById("overview").style.display="block";
			document.getElementById("lecturers").style.display="none";
			document.getElementById("ship").style.display="none";
			document.getElementById("rates").style.display="none";
			document.getElementById("program").style.display="none";	
			document.getElementById("itinerary").style.display="none";			
		}
		if (c=="lecture")
		{
			document.getElementById("overview").style.display="none";
			document.getElementById("lecturers").style.display="block";
			document.getElementById("ship").style.display="none";
			document.getElementById("rates").style.display="none";
			document.getElementById("program").style.display="none";	
			document.getElementById("itinerary").style.display="none";			
		}
		
		if (c=="ship")
		{
			document.getElementById("overview").style.display="none";
			document.getElementById("lecturers").style.display="none";
			document.getElementById("ship").style.display="block";
			document.getElementById("rates").style.display="none";
			document.getElementById("program").style.display="none";	
			document.getElementById("itinerary").style.display="none";			
		}
		
		if (c=="rates")
		{
			document.getElementById("overview").style.display="none";
			document.getElementById("lecturers").style.display="none";
			document.getElementById("ship").style.display="none";
			document.getElementById("rates").style.display="block";
			document.getElementById("program").style.display="none";	
			document.getElementById("itinerary").style.display="none";			
		}
		
		if (c=="program")
		{
			document.getElementById("overview").style.display="none";
			document.getElementById("lecturers").style.display="none";
			document.getElementById("ship").style.display="none";
			document.getElementById("rates").style.display="none";
			document.getElementById("program").style.display="block";
			document.getElementById("itinerary").style.display="none";				
		}
		if (c=="itinerary")
		{
		    
			document.getElementById("overview").style.display="none";
			document.getElementById("lecturers").style.display="none";
			document.getElementById("ship").style.display="none";
			document.getElementById("rates").style.display="none";
			document.getElementById("program").style.display="none";	
			document.getElementById("itinerary").style.display="block";	
			document.getElementById("itineraries").style.display="block";	
			
		    self.location.hash="bookmark";
		}
		
		
	}
	
