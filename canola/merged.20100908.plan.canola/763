             function validate_ticker()
              {
              var ticker = document.tickerbox.ticker.value;
              if( !isvalidticker( ticker ) )
              {
              alert( "Please enter a ticker symbol.");
              document.tickerbox.ticker.focus();
              return false;
              }
              return true;
              }
