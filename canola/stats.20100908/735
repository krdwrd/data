		  function buildDartTag(site, page, partner, regionID, height, width, tileNumber, date, spon, pos){
			var sz = width + "x" + height;
			var tag;

			tag = ' LANGUAGE=\"JavaScript1.1\" SRC= \'http://ad.doubleclick.net/adj/';
			tag += site + '/' + page + ';!category='+ partner + ';bp=' + partner;

            tag += ';ruid=' + regionID + ';';

			if (dcoptNum == "") {
                tag += 'dcopt=ist;';
            }
            tag += 'tile=' + tileNumber;

			if(pos != '')  {
				tag += ';pos=' + pos;
			}
			/*****************************
			LL-2213 -- Travel Spotlights: Dart Served
			Blair -- added a hack to look for the odd size of
            spotlights (1x1) and then plug in the spotlight number
            rather than the sponsor info
			******************************/
			if(spon != '')  {
				//tag += ';spon=' + spon;
				//alert(spon);
				if ((width == 1) && (height == 1)) {
			       //alert(spon + "xx");
			       tag += ';' + spon;
                } else {
                    tag += ';spon=' + spon;
                    //alert(spon);
                }
			}

			tag += ';sz=' + sz;
			tag += ';ord=' + date + ord;
			tag += '?\'></';

			tag = "<script" + tag + "script>";

			if ((!document.images && navigator.userAgent.indexOf("Mozilla2.") >= 0)
			     || navigator.userAgent.indexOf("WebTV") >= 0)
