var TcE_ = null;
function TgXMLHR() {
var Tx = false;
if (window.XMLHttpRequest) {
Tx = new XMLHttpRequest();
} else {
try
{
Tx = new ActiveXObject("Msxml2.XMLHTTP");
}
catch (ev)
{
try
{
Tx = new ActiveXObject("Microsoft.XMLHTTP");
}
catch (ev)
{
Tx = false;
}
}
}
return Tx;
}
function TeD() {
TcE_ = new TE();
TcE_.Tx = TgXMLHR();
if (TcE_.Tx) {
TcE_.Tx.open('POST', TsTS+'?tm=true', true);
