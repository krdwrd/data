<script language="JavaScript">
function applyLight()
{
// Applies the light filter to elementID
if ((navigator.appVersion.indexOf('MSIE') > 0)
     && (parseInt(navigator.appVersion) >= 4))
     {  var elementID = document.all.fadingCell;
        elementID.filters.Light.addPoint(0, 0, window.screen.width, 255, 255, 255, 100);  }
}
window.onload = new Function('applyLight()')
</script>

