	<script language="JavaScript" type="text/javascript">
		//when mouse over item
		function doMouseOver(obj)
		{
			obj.src = (obj.src).replace("_out", "_over");
			return;
		}
		//when mouse out item
		function doMouseOut(obj)
		{
			obj.src = (obj.src).replace("_over", "_out");
			return;
		}

		//pop the online version popup
		function popOnlineVersion(code)
		{
			window.open("/onlineGame.htm?code=" + code, "online", "width=780, height=535");
			return;
		}

	</script>
