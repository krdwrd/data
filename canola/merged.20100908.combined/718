<script type="text/javascript">

function omGoToCategory(newUrl) {
  window.location.href = newUrl;
  return false;
}

chosenCat = 0;
function SwitchMenu(obj){
	if(document.getElementById){
    var el = document.getElementById(obj);
    var ar = document.getElementById("cont").getElementsByTagName("DIV");
        if(el.style.display == "none"){
            for (var i=0; i<ar.length; i++){
                ar[i].style.display = "none";
            }
            el.style.display = "block";
        }else{
            el.style.display = "none";
        }
    }
    //close last icon opened
    if (chosenCat > 0)
	{
		chosenCatMenu = "menu" + chosenCat;
		chosenCatTd = "category_" + chosenCat;
		chosenCatTdIcon = "icon_category_" + chosenCat;
		if (document.getElementById) {
			document.getElementById(chosenCatMenu).className = "LeftSideleftbar_menuOut";
			document.getElementById(chosenCatTd).className = "LeftSideleftbar_category";
			document.getElementById(chosenCatTdIcon).className = "LeftSideleftbar_icon_category";
		}
	}
	//close this icon opened
    if (chosenCat == obj)
	{
		chosenCatMenu = "menu" + chosenCat;
		chosenCatTd = "category_" + chosenCat;
		chosenCatTdIcon = "icon_category_" + chosenCat;
		if (document.getElementById) { 
			document.getElementById(chosenCatMenu).className = "LeftSideleftbar_menuOut";
			document.getElementById(chosenCatTd).className = "LeftSideleftbar_category";
			document.getElementById(chosenCatTdIcon).className = "LeftSideleftbar_icon_category";
		}
		chosenCat = 0;
	}
	//open new icon
	else
	{
		chosenCat = obj;
		chosenCatMenu = "menu" + chosenCat;
		chosenCatTd = "category_" + chosenCat;
		chosenCatTdIcon = "icon_category_" + chosenCat;
		document.getElementById(chosenCatMenu).className = "LeftSideleftbar_menuOver";
		document.getElementById(chosenCatTd).className = "LeftSideleftbar_categoryChosen";
		document.getElementById(chosenCatTdIcon).className = "LeftSideleftbar_icon_categoryChosen";
	}
}
function ChangeClass(menu, menuNum, newClass) { 
     if (document.getElementById && chosenCat != menuNum) {
         document.getElementById(menu).className = newClass;
     } 
} 


document.onselectstart = new Function("return true");
</script>


